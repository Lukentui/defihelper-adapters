{
  "name": "defihelper-adapters",
  "version": "1.0.0",
  "description": "",
  "main": "./develop/server.js",
  "scripts": {
    "automates:lint:sol": "solhint 'automates/**/*.sol' && prettier -c 'automates/**/*.sol'",
    "automates:lint:sol:fix": "prettier --write 'automates/**/*.sol'",
    "automates:build": "hardhat compile",
    "build:adapters": "rollup -c adapters.rollup.js",
    "build-dev:adapters": "rollup -c adapters.rollup.js",
    "watch:adapters": "rollup -c adapters.rollup.js -w",
    "server": "node ./develop/server.js",
    "build:client": "parcel build ./develop/client/index.js -d public/dist --no-cache",
    "watch:client": "parcel watch ./develop/client/index.js -d public/dist",
    "dev": "npm-run-all --parallel watch:adapters watch:client server"
  },
  "author": "DeFi Helper",
  "license": "",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "hardhat": "^2.5.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.3.1",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "solc": "^0.8.6",
    "solhint": "^3.3.6",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@defihelper/ethers-multicall": "^0.3.0",
    "@openzeppelin/contracts": "^4.2.0",
    "axios": "^0.21.1",
    "bignumber.js": "^8.1.1",
    "dayjs": "^1.10.5",
    "ethers": "^5.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^5.2.0",
    "rollup": "^2.55.1",
    "tiny-glob": "^0.2.9"
  },
  "lint-staged": {
    "*": "prettier --write ."
  }
}
